<odoo>
  <data>
           <!--Display form view of wizard-->    
        <record id="view_dealer_report_wizard_form" model="ir.ui.view">
            <field name="name">dealer.report.wizard.form</field>
            <field name="model">dealer.report.wizard</field>
            <field name="arch" type="xml">
                <form string="Dealer Report Wizard">
                    <group>
                        
                       <group>
                            <field name="date_from" />
                            <field name="type" />
                      </group>
                        
                      <group>
                          <field name="date_to" />
                      </group>                        
                        
                    </group>
                    
                    <footer>
                        <button string='Print' name="check_report" type="object" default_focus="1" class="oe_highlight"/>
                        <button string="Cancel" class="btn-default" special="cancel"/>
                    </footer>
                </form>
            </field>
        </record>

        <!--Open the wizard on click of menu-->
        <record id="open_dealer_wizard_action" model="ir.actions.act_window">
            <field name="name">Dealer Report</field>
            <field name="res_model">dealer.report.wizard</field>
            <field name="view_mode">form</field>
            <field name="target">new</field>
            <field name="view_id" ref="view_dealer_report_wizard_form"/>
        </record>
      
       
       <menuitem id="menu_dealer_plot_details" name="Dealer Report" parent="menu_property_reporting"  action='open_dealer_wizard_action'
             sequence="10"/>
      
  </data>
</odoo>