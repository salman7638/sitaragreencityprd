<odoo>
  <data>
           <!--Display form view of wizard-->    
        <record id="view_commission_wizard_wizard_form" model="ir.ui.view">
            <field name="name">commission.wizard.form</field>
            <field name="model">commission.wizard</field>
            <field name="arch" type="xml">
                <form string="Commission Report">
                    <group>
                        
                       <group>
                              <field name="date" />
                      </group>
                        
                      <group>
                      </group>                        
                        
                    </group>
                    
                    <footer>
                        <button string='Print Xlsx Report' name="check_report" type="object" default_focus="1" class="oe_highlight"/>
<!--                         <button string='Print Pdf Report' name="pdf_report" type="object" default_focus="1" class="oe_highlight"/>
 -->
                        <button string="Cancel" class="btn-default" special="cancel"/>
                    </footer>
                </form>
            </field>
        </record>

        <!--Open the wizard on click of menu-->
        <record id="open_commission_wizard_action" model="ir.actions.act_window">
            <field name="name">Commision Report</field>
            <field name="res_model">commission.wizard</field>
            <field name="view_mode">form</field>
            <field name="target">new</field>
            <field name="view_id" ref="view_commission_wizard_wizard_form"/>
        </record>
      
       <menuitem id="menu_commission_report_status" name="Commission Report" parent="de_property_report.menu_property_reporting"  action='open_commission_wizard_action'
             sequence="13"/>
      
  </data>
</odoo>