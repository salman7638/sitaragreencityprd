<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data>
        <!-- inherit account budget form view -->
        <record id="crossovered_budget_view_form_inherit_cost_sheet" model="ir.ui.view">
            <field name="name">crossovered.budget.view.form.inherit.cost.sheet</field>
            <field name="model">crossovered.budget</field>
            <field name="inherit_id" ref="account_budget.crossovered_budget_view_form" />
            <field name="arch" type="xml">
                <xpath expr="//field[@name='analytic_account_id']" position="after">
                    <field name="project_cost_sheet_id" />
                    <field name="job_type_id" />
                </xpath>
                
            </field>
        </record>
        
        
    </data>
</odoo>