<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- Sequences for project cost sheet -->
        <record id="seq_project_cost_sheet" model="ir.sequence">
            <field name="name">Cost Sheet</field>
            <field name="code">project.cost.sheet</field>
            <field name="prefix">CS</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
        
        <!-- Sequences for project Job Order -->
        <record id="seq_project_task_job_order" model="ir.sequence">
            <field name="name">Job Order</field>
            <field name="code">project.task.job.order</field>
            <field name="prefix">JO</field>
            <field name="padding">5</field>
            <field name="company_id" eval="False"/>
        </record>
        
        <!-- Picking types -->
        <function model="res.company" name="create_missing_internal_requisition_sequence"/>
        <function model="res.company" name="create_missing_internal_requisition_picking_type"/>
        
        <function model="res.company" name="create_missing_internal_requisition_sequence"/>
        <function model="res.company" name="create_missing_internal_requisition_picking_type"/>

    </data>
</odoo>
